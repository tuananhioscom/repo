{
  "version": 2,
  "builds": [
    {
      "src": "server.tsx",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.tsx"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
